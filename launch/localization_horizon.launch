<launch>
<!-- Launch file for Livox Horizon LiDAR -->

	<arg name="rviz" default="true" />

	<rosparam command="load" file="$(find livox_localization)/config/horizon.yaml" />

	<param name="feature_extract_enable" type="bool" value="0"/>
	<param name="point_filter_num" type="int" value="1"/>
	<param name="dense_publish_enable" type="bool" value="1" />
	<param name="filter_size_surf" type="double" value="0.1" />
	<param name="filter_size_map" type="double" value="0.1" />
	<param name="cube_side_length" type="double" value="1000" />
	<param name="runtime_pos_log_enable" type="bool" value="0" />
	<param name="pcd_save_enable" type="bool" value="1" />
    <node pkg="livox_localization" type="fastlio_mapping" name="laserMapping" output="screen"  respawn='true'/>

	<arg name="map" default="/home/map/1008_2735/test_pcd_30.pcd" />
	<!-- loalization-->
	<!-- <node pkg="livox_localization" type="global_localization.py" name="global_localization" output="screen" respawn='true'/> -->

	<!-- transform  fusion-->
	<node pkg="livox_localization" type="transform_fusion.py" name="transform_fusion" output="screen" respawn='true'/>

	<!-- glbal map -->
	<node pkg="pcl_ros" type="pcd_to_pointcloud" name="map_publisher" output="screen"
		  args="$(arg map) 10 _frame_id:=/map cloud_pcd:=/map" />
    
	<!-- rosrun pcl_ros pcd_to_pointcloud /home/map/1008_2735/test_pcd_25.pcd 10 _frame_id:=/map cloud_pcd:=/map -->

	<group if="$(arg rviz)">
		<node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find livox_localization)/rviz_cfg/localization.rviz" />
	</group>

</launch>
